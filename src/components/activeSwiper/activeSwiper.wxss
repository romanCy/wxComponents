/* components/activeSwiper/activeSwiper.wxss */
.swiperClass {
    margin: 0;
    margin-top: 10px;
}
.slide-image {
    width: 100%;
    height: 90%;
    border-radius: 10px;
    position: relative;
}
image.active {
    transform: none;
    transition: all 1s ease-in 0s;
}
image.quiet {
    transform: scale(0.8);
    transition: all 1s ease-in 0s;
}